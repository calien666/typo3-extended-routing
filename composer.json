{
	"name": "calien/extended-routing",
	"type": "typo3-cms-extension",
	"description": "Extended Routing Mappers for TYPO3",
	"license": "MIT",
	"authors": [
		{
			"name": "Markus Hofmann",
			"role": "Developer",
			"email": "typo3@calien.de"
		}
	],
	"keywords": [
		"TYPO3",
		"Routing"
	],
	"extra": {
		"typo3/cms": {
			"extension-key": "extended_routing"
		}
	},
	"require": {
		"php": ">=7.4 || >= 8.0",
		"typo3/cms-core": "^11.5 || ^12.2"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3.0",
		"saschaegerer/phpstan-typo3": "^1.8",
		"phpstan/phpstan": "^1.3",
		"typo3/testing-framework": "dev-7-composerinstaller5-fix"
	},
	"autoload": {
		"psr-4": {
			"Calien\\ExtendedRouting\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Calien\\ExtendedRouting\\Tests\\": "Tests"
		}
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin",
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true,
			"sbuerk/typo3-cmscomposerinstallers-testingframework-bridge": true
		}
	}
}
